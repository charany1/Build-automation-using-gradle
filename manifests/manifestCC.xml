<?xml version="1.0" encoding="UTF-8"?>

<ExtensionManifest Version="4.0" ExtensionBundleId="com.zoom.ZmExtension" ExtensionBundleVersion=@version@
		ExtensionBundleName="ZmAdobeExtensions" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<ExtensionList>
		<Extension Id="com.zoom.ZmExtension.extension1" Version="1.0" />
	</ExtensionList>
	<ExecutionEnvironment>
		<HostList>
			<Host Version="[17.0,17.9]" Name="ILST" />
			<Host Version="[14.0,14.9]" Name="PHXS" />
			<Host Version="[13.1,13.9]" Name="FLPR" />
			<Host Version="[14.0,14.9]" Name="PHSP" />
			<Host Version="[2.0,2.9]" Name="PRLD" />
			<Host Version="[9.1,9.9]" Name="IDSN" />
			<Host Version="[7.0,7.9]" Name="PPRO" />
		</HostList>
		<LocaleList>
			<Locale Code="All" />
		</LocaleList>
		<RequiredRuntimeList>
			<RequiredRuntime Name="CSXS" Version="4.0" />
		</RequiredRuntimeList>
	</ExecutionEnvironment>
	<DispatchInfoList>
		<Extension Id="com.zoom.ZmExtension.extension1">
			<DispatchInfo >
				<Resources>
				<MainPath>./index.html</MainPath>
				<ScriptPath>./ZmAdobeExtensions.jsx</ScriptPath>
				</Resources>
				<Lifecycle>
					<AutoVisible>true</AutoVisible>
				</Lifecycle>
				<UI>
					<Type>Panel</Type>
					<Menu>Evolphin Zoom</Menu>
					<Geometry>
						<Size>
							<Height>%height</Height>
							<Width>%width</Width>
						</Size>
						<MinSize>
							<Height>%height</Height>
							<Width>%width</Width>
						</MinSize>
					</Geometry>
					<Icons>
						<Icon Type="Normal">./assets/icon_normal.png</Icon>
						<Icon Type="RollOver">./assets/icon_normal.png</Icon>
					</Icons>
				</UI>
			</DispatchInfo>
		</Extension>
	</DispatchInfoList>
</ExtensionManifest>